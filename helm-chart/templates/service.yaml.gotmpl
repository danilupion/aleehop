apiVersion: v1
kind: Service
metadata:
  name: {{ include "aleehop-web.fullname" . }}
  labels:
    {{- include "aleehop-web.labels" . | nindent 4 }}
spec:
  type: {{ .Values.service.type }}
  ports:
    - port: {{ .Values.service.port }}
      targetPort: http
      protocol: TCP
      name: http
  selector:
    app: {{ include "aleehop-web.fullname" . }}
